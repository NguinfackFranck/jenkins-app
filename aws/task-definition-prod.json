{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "LearnJenkinsApp",
            "image": "140023385864.dkr.ecr.us-east-1.amazonaws.com/babsom/learnjenkinsapp:#APP_VERSION#",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": ".25 vCPU",
    "executionRoleArn": "arn:aws:iam::140023385864:role/ecsTaskExecutionRole"
}